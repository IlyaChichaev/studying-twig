<tr>
    {% for heading_name in table_heading %}
        {% if loop.index == 1 %}
            <th onclick="sort_table({{ loop.index - 1 }})" , data-sort="-1">{{ loop.parent.loop.index }}{{ heading_name|raw }}</th>
        {% else %}
            <th onclick="sort_table({{ loop.index - 1 }})" , data-sort="1">{{ loop.parent.loop.index }}{{ heading_name|raw }}</th>
        {% endif %}
    {% endfor %}
</tr>